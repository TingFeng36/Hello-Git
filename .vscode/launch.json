{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: 配置项目",
      "cmakeDebugType": "configure",
      "clean": false,
      "configureAll": false
    },
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: 脚本调试",
      "cmakeDebugType": "script",
      "scriptPath": "${workspaceFolder}/<...>.cmake"
    },
    {
      "type": "cmake",
      "request": "launch",
      "name": "CMake: 外部启动",
      "cmakeDebugType": "external",
      "pipeName": "<...>"
    },
    {
      "name": "PowerShell: Binary Module Pester Tests",
      "type": "PowerShell",
      "request": "launch",
      "script": "Invoke-Pester",
      "createTemporaryIntegratedConsole": true,
      "attachDotnetDebugger": true
    },

    {
      "name": "(Windows) 附加",
      "type": "cppvsdbg",
      "request": "attach",
    },
    {
      "name": "(gdb) 管道附加",
      "type": "cppdbg",
      "request": "attach",
      "program": "输入程序名称，例如 ${workspaceFolder}/a.exe",
      "processId": "${command:pickRemoteProcess}",
      "pipeTransport": {
        "debuggerPath": "/usr/bin/gdb",
        "pipeProgram": "<管道程序的完整路径，如 plink.exe>",
        "pipeArgs": [],
        "pipeCwd": ""
      },
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "为 gdb 启用整齐打印",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "将反汇编风格设置为 Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) 管道启动",
      "type": "cppdbg",
      "request": "launch",
      "program": "输入程序名称，例如 ${workspaceFolder}/a.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "pipeTransport": {
        "debuggerPath": "/usr/bin/gdb",
        "pipeProgram": "<管道程序的完整路径，如 plink.exe>",
        "pipeArgs": [],
        "pipeCwd": ""
      },
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "为 gdb 启用整齐打印",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "将反汇编风格设置为 Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "(gdb) 附加",
      "type": "cppdbg",
      "request": "attach",
      "program": "输入程序名称，例如 ${workspaceFolder}/a.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "/path/to/gdb",
      "setupCommands": [
          {
              "description": "为 gdb 启用整齐打印",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "将反汇编风格设置为 Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    },
